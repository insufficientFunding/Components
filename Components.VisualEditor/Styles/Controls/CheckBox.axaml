<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Design.PreviewWith>
		<Border Padding="20">
			<CheckBox />
		</Border>
	</Design.PreviewWith>

	<Style Selector="CheckBox">
		<Style Selector="^ /template/ ToggleButton">
			<Setter Property="BorderBrush" Value="{DynamicResource BackgroundLower}"></Setter>
		</Style>
	</Style>
</Styles>